; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="漫画阅读器"
config/description="一款离线的漫画阅读APP"
config/version="1.0.0"
config/tags=PackedStringArray("git", "漫画阅读器")
run/main_scene="res://场景/主场景.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="BookRead"
config/auto_accept_quit=false
config/quit_on_go_back=false
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(1, 1, 1, 1)
boot_splash/image="res://素材/IconParkBook.png"
config/icon="res://素材/图标.png"

[autoload]

Data="*res://脚本/data.gd"

[display]

window/size/viewport_width=540
window/size/viewport_height=1200
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=1

[global_group]

"漫画"=""

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
